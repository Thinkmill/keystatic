    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "ts"
  ],
  "dependencies": {
    "@babel/runtime": "^7.18.3",
    "@keystar-ui/style": "^0.1.3",
    "@keystar-ui/types": "^0.1.1",
    "@react-aria/focus": "^3.10.0",
    "@react-aria/utils": "^3.14.2",
    "@react-aria/visually-hidden": "^3.6.1",
    "emery": "^1.4.1",
    "react-keyed-flatten-children": "^1.3.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.25",
    "react": "^18.2.0"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "engines": {
    "node": ">= 14.13"
  },
  "preconstruct": {
    "entrypoints": [
      "index.ts",
      "ts/index.ts"
    ]
