{
  "name": "@voussoir/utils",
  "version": "1.0.0",
  "description": "Utility hooks, types, and components for Voussoir.",
  "license": "MIT",
  "main": "dist/voussoir-utils.cjs.js",
  "module": "dist/voussoir-utils.esm.js",
  "exports": {
    ".": {
      "module": "./dist/voussoir-utils.esm.js",
      "default": "./dist/voussoir-utils.cjs.js"
    },
    "./ts": {
      "module": "./ts/dist/voussoir-utils-ts.esm.js",
      "default": "./ts/dist/voussoir-utils-ts.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "ts"
  ],
  "dependencies": {
    "@babel/runtime": "^7.18.3",
    "@react-aria/focus": "^3.10.0",
    "@react-aria/utils": "^3.14.2",
    "@react-aria/visually-hidden": "^3.6.1",
    "@voussoir/style": "^0.0.0",
    "@voussoir/types": "^0.0.0",
    "emery": "^1.4.1",
    "lodash": "^4.17.21",
    "react-keyed-flatten-children": "^1.3.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.25",
    "react": "^18.2.0"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "engines": {
    "node": ">= 14.13"
  },
  "preconstruct": {
    "entrypoints": [
      "index.ts",
      "ts/index.ts"
    ]
  }
}
